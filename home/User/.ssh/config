Host dev
  IgnoreUnknown UseKeychain
  #  UseKeychain yes
  HostName 1.2.3.4
  User admin
  Port 123
  AddKeysToAgent yes
  ForwardX11 yes 
  ForwardX11Trusted yes 
  ForwardAgent yes 
  IdentityFile ~/.ssh/1_id
